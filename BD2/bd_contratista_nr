CREATE TABLE public.contratistas_nr (
	id serial NOT NULL,
	user_id int4 NULL,
	edocontratista_id int4 NOT NULL,
	objcontratista_id int4 NULL,
	nivelfinanciero_id int4 NULL,
	racoficina_id int4 NULL,
	tipocontratista bpchar(2) NULL,
	estado_id int4 NULL,
	ciudade_id int4 NULL,
	municipio_id int4 NULL,
	parroquia_id int4 NULL,
	rifced varchar(20) NOT NULL,
	nombre varchar(255) NULL,
	tipopersona bpchar(1) NULL,
	dencomerciale_id int4 NOT NULL,
	ultprocaprob int8 NULL,
	procactual int8 NULL,
	dirfiscal text NULL,
	percontacto varchar(60) NULL,
	telf1 varchar(50) NULL,
	fecactsusc_at date NULL,
	fecvencsusc_at date NULL,
	fecinscrnc_at date NULL,
	fecvencrnc_at date NULL,
	numcertrnc varchar(20) NULL,
	numcontrol_certrnc varchar(20) NULL,
	contimp_certrnc int4 NULL,
	contimp_copiarnc int4 NULL,
	codedocont bpchar(2) NULL,
	loginant varchar(60) NULL,
	fecvencrechazo_at date NULL,
	recibido int4 NULL,
	CONSTRAINT pk_contratistas_nr PRIMARY KEY (id),
	CONSTRAINT fk_contrati_rl_estado_estados FOREIGN KEY (estado_id) REFERENCES estados(id),
	CONSTRAINT fk_contrati_rl_munici_municipi FOREIGN KEY (municipio_id) REFERENCES municipios(id),
	CONSTRAINT fk_contrati_rl_parroq_parroqui FOREIGN KEY (parroquia_id) REFERENCES parroquias(id),
	CONSTRAINT fk_contrati_rlcedocon_edocontr FOREIGN KEY (edocontratista_id) REFERENCES edocontratistas(id)
);
